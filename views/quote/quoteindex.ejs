<h1>Quote Page</h1>
<div>
    <% quotes.forEach(function(quote){ %>
    <div>
        <a href="/quote/<%= quote._id%>"><%= quote.quote %></a>
    </div>
    <div> Auther: <%= quote.auther %></div>
    <div> by: <%= quote.quoteUser %></div>
    <hr>
    <div>Created On: <%= moment(quote.createdAt).fromNow() %></div>
    <div>
        <form method="POST" action="/quote/<%= quote._id %>/delete?_method=DELETE">
            <button type="submit">Delete Quote</button>
        </form>
    </div>
    <% }) %>
</div>